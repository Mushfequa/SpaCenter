{
  "name": "nodemvc",
  "version": "0.2.1",
  "private": false,
  "scripts": {
    "rundev": "nodemon --watch views/ --watch config/ --watch controllers/ --watch data/ --watch locales/ --watch services/ --watch modules/ --watch node_modules/ --watch app.js app.js",
    "start": "node app.js"
  },
  "bin": {
    "nodemvc": "bin/nodemvc.bin.js"
  },
  "dependencies": {
    "express": "~4.10.6",
    "jade": "~1.8.2",
    "jade-helpers": "*",
    "stylus": "*",
    "validator": "*",
    "passport": "0.2.0",
    "passport-local": "0.1.6",
    "express-validator": "*",
    "sequelize": "1.7.10",
    "pg": "~2.8.4",
    "async": "*",
    "uglify-js": "*",
    "qmessage": "*",
    "i18next": "*",
    "mocha": "*",
    "grunt-mocha-test": "*",
    "should": "*",
    "sequelize-fixtures": "*",
    "blanket": "*",
    "zwe-uglifyjs-middleware": "*",
    "grunt": "*",
    "pm2": "*",
    "lodash": ">=2.4.1",
    "minimist": "~1.1.0",
    "fs-extra": "~0.13.0",
    "npm": "~2.1.16",
    "serve-favicon": "~2.2.0",
    "method-override": "~2.3.1",
    "morgan": "~1.5.1",
    "express-session": "~1.9.3",
    "body-parser": "~1.10.1",
    "errorhandler": "~1.3.2",
    "multer": "~0.1.6",
    "cookie-parser": "~1.3.3"
