  },
  "devDependencies": {
    "mocha": "~2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsguy/nodemvc.git"
  },
  "keywords": [
    "mvc",
    "framework",
    "opinionated",
    "useful",
    "jsguy"
